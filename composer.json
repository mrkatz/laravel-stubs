{
  "name": "mrkatz/laravel-stubs",
  "description": "Laravel Stubs Editor & Generator",
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Kaczocha",
      "email": "adamkaczocha@gmail.com"
    }
  ],
  "homepage": "https://github.com/mrkatz/laravel-stubs",
  "keywords": [
    "Laravel",
    "Stubs",
    "Generator"
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1|^7.2"
  },
  "require-dev": {
      "orchestra/testbench": "^3.8|^4.0"
  },
  "autoload": {
    "psr-4": {
      "Mrkatz\\LaravelStubs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mrkatz\\LaravelStubs\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mrkatz\\LaravelStubs\\Providers\\ConsoleSupportServiceProvider"
      ]
    }
  }
}
